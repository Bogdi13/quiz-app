<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.example.ProiectAPI.domain">

    <class name="com.example.ProiectAPI.domain.model.QuestionQuiz" table="QuestionQuizes">
        <composite-id>
            <key-property name="id" type = "java.lang.Integer" column="id"/>
        </composite-id>
        <property name="questionId" type = "java.lang.String" column="questionId"/>
        <property name="quizId" type = "java.lang.String" column="quizId"/>
    </class>

</hibernate-mapping>